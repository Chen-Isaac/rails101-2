<div class="col-md-4 col-md-offset-4">
  <h2>add new discussion board</h2>

  <ul>
    <li><%= "Don't make mistake "%></li>
  </ul>

  <hr>
  <%= form_for @group do |f| %>
  <% if @group.errors.any? %>
  <ul>
    <% @group.errors.full_messages.each do |msgfuck| %>
      <li><%= msgfuck %></li>
      <li><%= "You fucking fool "%></li>
    <% end %>
  </ul>
  <% end %>

  title@new
  <%= f.text_field :title %>
  <br>
  <br>
  description@new
  <br>
  <br>
  <%= f.text_area :description %>
  <br>

  <%= f.submit "Submit", :disable_with => 'Submiting...' %>
  <% end %>

</div>
